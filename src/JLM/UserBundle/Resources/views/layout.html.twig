{%  extends "::layout.html.twig" %}

{% block container %}
<div class="container">
  {% if twbs == 2 %}
    <div class="span3"></div>
    <div class="span6">
      <div class="page-header">
        <h1>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<img src="{{ asset('bundles/jlmuser/img/jlm-logo.png') }}"></h1>
      </div>
  {% else %}
    <div class="row">
	  <div class="col-md-12">
  {% endif %}
      {% for type, messages in app.session.flashbag.all() %}
        {% for message in messages %}
          <div class="{{ type }}">{{ message }}</div>
        {% endfor %}
      {% endfor %}
   {% if twbs == 2 %}
      <div class="well">
     {% else %}
     </div>
    </div>
    {% endif %}
        {% block fos_user_content %}
        {% endblock fos_user_content %}
      
      </div>
      {% if twbs == 2 %}
    </div>
	<div class="span3"></div>
	{% endif %}
{% endblock %}