<h1>InterventionPlanned</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Creation</th>
            <td>{{ entity.creation|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Contactname</th>
            <td>{{ entity.contactName }}</td>
        </tr>
        <tr>
            <th>Contactphones</th>
            <td>{{ entity.contactPhones }}</td>
        </tr>
        <tr>
            <th>Contactemail</th>
            <td>{{ entity.contactEmail }}</td>
        </tr>
        <tr>
            <th>Reason</th>
            <td>{{ entity.reason }}</td>
        </tr>
        <tr>
            <th>Priority</th>
            <td>{{ entity.priority }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('interventionplanned') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('interventionplanned_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('interventionplanned_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
