<h1>Contract</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Number</th>
            <td>{{ entity.number }}</td>
        </tr>
        <tr>
            <th>Begin</th>
            <td>{{ entity.begin|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Endwarranty</th>
            <td>{{ entity.endWarranty|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>End</th>
            <td>{{ entity.end|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Turnover</th>
            <td>{{ entity.turnover }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('contract') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('contract_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('contract_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
