<h1>DocumentContentLine list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Reference</th>
            <th>Designation</th>
            <th>Quantity</th>
            <th>Vat</th>
            <th>Unitprice</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('documentcontentline_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.reference }}</td>
            <td>{{ entity.designation }}</td>
            <td>{{ entity.quantity }}</td>
            <td>{{ entity.vat }}</td>
            <td>{{ entity.unitPrice }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('documentcontentline_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('documentcontentline_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('documentcontentline_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
