{%  extends "::layout.html.twig" %}

{% block container %}
		<div class="container">
			<div class="span3"></div>
			<div class="span6">
				<div class="page-header">
					<h1>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<img src="{{ asset('bundles/jlmuser/img/jlm-logo.png') }}"></h1>
				</div>
        {% for key, message in app.session.getFlashes() %}
        <div class="{{ key }}">
            {{ message|trans({}, 'FOSUserBundle') }}
        </div>
        {% endfor %}

        <div>
            {% block fos_user_content %}
            {% endblock fos_user_content %}
        </div>
        </div>
	        <div class="span3"></div>
        </div>
{% endblock %}