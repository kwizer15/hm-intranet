{% extends "JLMDailyBundle::layout.html.twig" %}

{% block content %}
<h1>Editer une astreinte</h1>
<form action="{{ path('standby_update',{'id':entity.id}) }}" method="post" {{ form_enctype(form) }} class="form-horizontal">
	<fieldset>
		{{ form_widget(form) }}
	    <div class="form-actions">
		   <button type="submit" class="btn btn-primary">{{ 'pencil'|icon(true) }} Enregistrer</button>
		</div>
	</fieldset>
</form>
{% endblock %}
