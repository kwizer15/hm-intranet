CREATE TABLE transmitter_models (id INT AUTO_INCREMENT NOT NULL, name VARCHAR(255) NOT NULL, product_reference VARCHAR(10) NOT NULL, PRIMARY KEY(id)) DEFAULT CHARACTER SET utf8 COLLATE utf8_unicode_ci ENGINE = InnoDB;
CREATE TABLE transmitter_user_groups (id INT AUTO_INCREMENT NOT NULL, site_id INT DEFAULT NULL, model_id INT DEFAULT NULL, name VARCHAR(255) NOT NULL, INDEX IDX_4385B9CAF6BD1646 (site_id), INDEX IDX_4385B9CA7975B7E7 (model_id), PRIMARY KEY(id)) DEFAULT CHARACTER SET utf8 COLLATE utf8_unicode_ci ENGINE = InnoDB;
CREATE TABLE transmitters (id INT AUTO_INCREMENT NOT NULL, number BIGINT NOT NULL, firstName VARCHAR(255) DEFAULT NULL, lastName VARCHAR(255) DEFAULT NULL, parkingPlace VARCHAR(5) DEFAULT NULL, appartment VARCHAR(5) DEFAULT NULL, unprogramed DATE DEFAULT NULL, userGroup_id INT DEFAULT NULL, attributionList_id INT DEFAULT NULL, replacedBy_id INT DEFAULT NULL, INDEX IDX_C50004972B674466 (userGroup_id), INDEX IDX_C500049767B32725 (attributionList_id), UNIQUE INDEX UNIQ_C5000497A247C6F4 (replacedBy_id), PRIMARY KEY(id)) DEFAULT CHARACTER SET utf8 COLLATE utf8_unicode_ci ENGINE = InnoDB;
CREATE TABLE transmitter_attributions (id INT AUTO_INCREMENT NOT NULL, bill_id INT DEFAULT NULL, creation DATE NOT NULL, UNIQUE INDEX UNIQ_67C3CBCA1A8C12F5 (bill_id), PRIMARY KEY(id)) DEFAULT CHARACTER SET utf8 COLLATE utf8_unicode_ci ENGINE = InnoDB;
ALTER TABLE transmitter_user_groups ADD CONSTRAINT FK_4385B9CAF6BD1646 FOREIGN KEY (site_id) REFERENCES sites (id);
ALTER TABLE transmitter_user_groups ADD CONSTRAINT FK_4385B9CA7975B7E7 FOREIGN KEY (model_id) REFERENCES transmitter_models (id);
ALTER TABLE transmitters ADD CONSTRAINT FK_C50004972B674466 FOREIGN KEY (userGroup_id) REFERENCES transmitter_user_groups (id);
ALTER TABLE transmitters ADD CONSTRAINT FK_C500049767B32725 FOREIGN KEY (attributionList_id) REFERENCES transmitter_attributions (id);
ALTER TABLE transmitters ADD CONSTRAINT FK_C5000497A247C6F4 FOREIGN KEY (replacedBy_id) REFERENCES transmitters (id);
ALTER TABLE transmitter_attributions ADD CONSTRAINT FK_67C3CBCA1A8C12F5 FOREIGN KEY (bill_id) REFERENCES bill (id)
