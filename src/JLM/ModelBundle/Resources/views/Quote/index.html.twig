<h1>Quote list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Creation</th>
            <th>Trusteename</th>
            <th>Trusteestreet</th>
            <th>Trusteezip</th>
            <th>Trusteecity</th>
            <th>Trusteeinterlocutor</th>
            <th>Paymentrules</th>
            <th>Deliveryrules</th>
            <th>Customercomments</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('quote_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{% if entity.creation %}{{ entity.creation|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.trusteeName }}</td>
            <td>{{ entity.trusteeStreet }}</td>
            <td>{{ entity.trusteeZip }}</td>
            <td>{{ entity.trusteeCity }}</td>
            <td>{{ entity.trusteeInterlocutor }}</td>
            <td>{{ entity.paymentRules }}</td>
            <td>{{ entity.deliveryRules }}</td>
            <td>{{ entity.customerComments }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('quote_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('quote_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('quote_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
