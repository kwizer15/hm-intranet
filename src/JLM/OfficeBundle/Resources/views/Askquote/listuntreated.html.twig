{% extends "JLMOfficeBundle:Askquote:index.html.twig" %}

{% block pagination %}
<li{% if page == 1 %} class="disabled"{% endif %}><a href="{{ path('askquote_listuntreated_page',{ 'page' : page-1 }) }}">«</a></li>
{% for p in range(1,nbPages) %}
<li{% if p == page %} class="active"{% endif %}><a href="{{ path('askquote_listuntreated_page',{ 'page' : p }) }}">{{ p }}</a></li>
{% endfor %}
<li{% if page == nbPages %} class="disabled"{% endif %}><a href="{{ path('askquote_listuntreated_page',{ 'page' : page+1 }) }}">»</a></li>
{% endblock %}