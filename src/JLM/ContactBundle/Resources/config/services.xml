<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

	<services>
    	<service id="jlm_contact.type.hiddencontacttype" class="JLM\ContactBundle\Form\Type\HiddenContactType">
			<argument type="service" id="doctrine.orm.entity_manager"/>
   			<tag name="form.type" alias="hiddencontacttype" />
		</service>
		<service id="jlm_contact.type.selectcontacttype" class="JLM\ContactBundle\Form\Type\SelectContactType">
			<argument type="service" id="doctrine.orm.entity_manager"/>
   			<tag name="form.type" alias="jlm_contact_selectcontacttype" />
		</service>
	</services>
</container>
